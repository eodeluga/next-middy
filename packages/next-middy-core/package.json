{
  "name": "next-middy-core",
  "version": "1.0.0",
  "description": "Core engine for next-middy: typed middleware composition, structured error handling, and Next.js integration.",
  "author": "Eugene Odeluga <eodeluga@users.noreply.github.com> (https://github.com/eodeluga)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist ../../.tsbuildinfo/core.tsbuildinfo",
    "test": "vitest run"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^22.7.5"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/error.middle.d.ts",
      "import": "./dist/middleware/error.middle.js"
    },
    "./errors": {
      "types": "./dist/errors/enriched.error.d.ts",
      "import": "./dist/errors/enriched.error.js"
    },
    "./utils": {
      "types": "./dist/utils/next-middy.util.d.ts",
      "import": "./dist/utils/next-middy.util.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eodeluga/next-middy"
  },
  "bugs": {
    "url": "https://github.com/eodeluga/next-middy/issues"
  },
  "homepage": "https://github.com/eodeluga/next-middy#readme",
  "keywords": [
    "nextjs",
    "middleware",
    "typescript",
    "api",
    "error-handling"
  ],
  "publishConfig": {
    "access": "public"
  }
}
