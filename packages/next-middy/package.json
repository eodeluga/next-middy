{
  "name": "next-middy",
  "version": "1.0.0",
  "description": "Type-safe, composable middleware framework for Next.js API routes with optional Zod integration.",
  "author": "Eugene Odeluga <eodeluga@users.noreply.github.com> (https://github.com/eodeluga)",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepack": "cp ../../README.md . && cp ../../LICENSE . && npm run build",
    "postpack": "rm -f README.md LICENSE",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "next": ">=13.0.0 <18.0.0",
    "zod": ">=3.0.0 <5.0.0"
  },
  "devDependencies": {
    "typescript": "5.6.3",
    "@types/node": "24.9.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./zod": {
      "types": "./dist/zod/index.d.ts",
      "import": "./dist/zod/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "core": ["dist/core/index.d.ts"],
      "zod": ["dist/zod/index.d.ts"]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eodeluga/next-middy"
  },
  "bugs": {
    "url": "https://github.com/eodeluga/next-middy/issues"
  },
  "homepage": "https://github.com/eodeluga/next-middy#readme",
  "keywords": [
    "nextjs",
    "middleware",
    "typescript",
    "zod",
    "error-handling",
    "serverless"
  ],
  "publishConfig": {
    "access": "public"
  }
}
