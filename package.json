{
  "name": "next-middy-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Type-safe, composable middleware framework for Next.js API routes, with optional Zod integration.",
  "author": "Eugene Odeluga <eodeluga@users.noreply.github.com> (https://github.com/eodeluga)",
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "packages/next-middy-core",
    "packages/next-middy"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npx tsc -b",
    "clean": "rm -rf packages/**/dist .tsbuildinfo",
    "build:core": "npm run build -w packages/next-middy-core",
    "build:main": "npm run build -w packages/next-middy",
    "prepare": "npm run build",
    "test": "vitest run"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eodeluga/next-middy"
  },
  "bugs": {
    "url": "https://github.com/eodeluga/next-middy/issues"
  },
  "homepage": "https://github.com/eodeluga/next-middy#readme",
  "keywords": [
    "nextjs",
    "middleware",
    "typescript",
    "zod",
    "error-handling",
    "serverless"
  ]
}
